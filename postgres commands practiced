select * from table_1;

select all * from table_1;

select distinct user_id, email_id from table_1;

select pk, user_id from table_1;

select all pk, user_id from table_1;

insert into table_1 (user_id, email_id) values('isw115', 'mas@gmail.com');

select * from table_1 where user_id = 'isw144';

select * from table_1 order by user_id asc;

select * from table_1 order by user_id limit 5 offset 1 ;

select user_id from table_1 group by user_id;


SELECT user_id, COUNT(*) AS count
FROM table_1
GROUP BY user_id
HAVING COUNT(*) = 1;
